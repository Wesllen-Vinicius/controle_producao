{
  "expo": {
    "name": "Producao & Estoque",
    "slug": "producao-estoque-app",
    "scheme": "prodest",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0b1020"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0b1020"
      }
    },
    "web": {
      "bundler": "metro"
    }
  }
}
